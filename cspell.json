{
  "ignorePaths": [
    "dist/",
    "**/node_modules/**",
    "prek/**",
    "**/*.drawio",
    "**/*.svg"
  ],
  "language": "en",
  "noConfigSearch": true,
  "allowCompoundWords": true,
  "words": [
    "Abhi",
    "Anchore",
    "cyclonedx",
    "GHSA",
    "ghsas",
    "hmpps",
    "Markan",
    "NTIA",
    "OCTO",
    "ossf",
    "pipenv",
    "prek",
    "pycache",
    "SARIF",
    "SLSA",
    "SSDF",
    "syft",
    "unindexed",
    "venv"
  ],
  "dictionaries": [
    "en-gb",
    "companies",
    "softwareTerms",
    "misc",
    "lorem-ipsum",
    "typescript",
    "node",
    "bash",
    "npm"
  ],
  "languageSettings": [
    {
      "languageId": "commit-msg",
      "ignoreRegExpList": [
        "/^#.*/gm"
      ]
    }
  ],
  "version": "0.2"
}
