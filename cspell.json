{
  "ignorePaths": ["**/node_modules/**", "prek/**", "**/*.drawio", "**/*.svg"],
  "language": "en",
  "noConfigSearch": true,
  "allowCompoundWords": true,
  "words": [
    "Abhi",
    "Anchore",
    "cyclonedx",
    "Markan",
    "NTIA",
    "ossf",
    "prek",
    "pycache",
    "SARIF",
    "syft",
    "unindexed",
    "venv",
    "SLSA",
    "SSDF",
    "pipenv"
  ],
  "dictionaries": [
    "en-gb",
    "companies",
    "softwareTerms",
    "misc",
    "lorem-ipsum",
    "typescript",
    "node",
    "bash",
    "npm"
  ],
  "languageSettings": [
    {
      "languageId": "commit-msg",
      "ignoreRegExpList": ["/^#.*/gm"]
    }
  ],
  "version": "0.2"
}
